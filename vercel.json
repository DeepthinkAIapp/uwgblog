{
  "version": 2,
  "builds": [
    {
      "src": "blog/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/blog/index.html"
    },
    {
      "src": "/blog/(.*)",
      "dest": "/blog/$1"
    }
  ],
  "outputDirectory": "blog",
  "public": true
} 